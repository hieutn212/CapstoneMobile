<android.support.v4.widget.SwipeRefreshLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeLayout"
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.project.mobilecapstone.Fragment.MapTrackingFragment">

    <FrameLayout
    android:id="@+id/canvasView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/btn_track_path"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end|bottom"
            android:clickable="true"
            android:src="@drawable/ic_near_me_white_48dp"
            app:backgroundTint="@color/colorAccent"
            android:layout_margin="16dp"/>
    </FrameLayout>


</android.support.v4.widget.SwipeRefreshLayout>